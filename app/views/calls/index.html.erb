<h1>Listing calls</h1>

<table>
  <tr>
    <th>Sid</th>
    <th>From</th>
    <th>To</th>
    <th>Status</th>
    <th>Direction</th>
    <th>Name</th>
  </tr>

<% for call in @calls %>
  <tr>
    <td><%=h call.sid %></td>
    <td><%=h call.from %></td>
    <td><%=h call.to %></td>
    <td><%=h call.status %></td>
    <td><%=h call.direction %></td>
    <td><%=h call.name %></td>
    <td><%= link_to 'Show', call %></td>
    <td><%= link_to 'Edit', edit_call_path(call) %></td>
    <td><%= link_to 'Destroy', call, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New call', new_call_path %>
