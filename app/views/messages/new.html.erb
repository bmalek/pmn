<h1>New cell phone</h1>

<% form_for(@message) do |f| %>
  <%= f.error_messages %>
  
  <p>
    <%= f.label :number %><br />
    <%= f.text_field :from %>
  </p>
  <p>
    <%= f.label :body %><br />
    <%= f.text_area :body %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', messages_path %>
